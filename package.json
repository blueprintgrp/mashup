{
  "name": "mashup-lang",
  "version": "0.4.3",
  "state": "stable",
  "description": "Interpreted scripting language.",
  "main": "./src/index.ts",
  "bin": {
    "mashup": "./src/index.js"
  },
  "scripts": {
    "dev": "ts-node .",
    "build": "tsc && pkg .",
    "push": "git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueprintgrp/mashup.git"
  },
  "author": "blueprintgrp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/blueprintgrp/mashup/issues"
  },
  "homepage": "https://github.com/blueprintgrp/mashup#readme",
  "dependencies": {
    "@types/node": "^20.2.5",
    "readline-sync": "^1.4.10",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  },
  "pkg": {
    "scripts": "build/lib/**/.js",
    "outputPath": "build/bin",
    "targets": [
      "node18-linux-x64",
      "node18-win-x64"
    ]
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.6.7"
  },
  "directories": {
    "example": "examples"
  }
}
